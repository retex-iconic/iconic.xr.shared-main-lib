{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "maven": {
    "enabled": true,
    "registryUrls": [
      "https://repo.maven.apache.org/maven2",
      "https://maven.pkg.github.com/retex-iconic/iconic.xr.suite"
    ]
  },
  "hostRules": [
    {
      "hostType": "maven",
      "matchHost": "https://maven.pkg.github.com/retex-iconic/iconic.xr.suite",
      "username": "nomed",
      "encrypted": {
        "token": ""
      }
    }
  ]
}